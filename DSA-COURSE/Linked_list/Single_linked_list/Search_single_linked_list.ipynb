   "source": [
    "class Node:\n",
    "    def __init__(self, data) -> None:\n",
    "        self.data = data\n",
    "        self.next = None\n",
    "\n",
    "n1 = Node(\"Karachi\")\n",
    "n2 = Node([1, 2, 3, 4])\n",
    "n3 = Node({\"English\":5})\n",
    "n4 = Node(8)\n",
    "\n",
    "n1.next= n2\n",
    "n2.next = n3\n",
    "n3.next = n4\n",
    "\n",
    "print(n1.next.next.next.data)\n",
    "\n"
   ]
  }
